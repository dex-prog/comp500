#include <stdio.h>
enum Hero {
    AQUAMAN, 
    BATMAN, 
    CYBORG,
    SUPERMAN,
    THE_FLASH,
    WONDER_WOMAN,   
    BLACK_WIDOW,
    CAPTAIN_AMERICA,
    HAWKEYE,
    IRONMAN,
    SPIDERMAN,
    THE_HULK
    
    };

void print_hero (enum Hero hero);
    
int main (){

    enum Hero hero_names;
   
    for (int i = AQUAMAN; i <= WONDER_WOMAN; i++) {
        hero_names = i;
        print_hero(hero_names);
        printf("DC\n\n");
    }
    for (int j = BLACK_WIDOW; j <= THE_HULK; j++) {
        hero_names = j;
        print_hero(hero_names);
        printf("Marvel\n\n");
    }
            

    //printf("Hello, World!\n");
    return 0;
}

void print_hero (enum Hero hero){
     switch (hero) {
        case AQUAMAN:
            printf("Aquaman\n");
            break;
        case BATMAN:
            printf("Batman\n");
            break;
        case WONDER_WOMAN:
            printf("Wonder Woman\n");
            break;
        case CYBORG:
            printf("Cyborg\n");
            break;
        case SUPERMAN:      
            printf("Superman\n");
            break;
        case THE_FLASH:
            printf("The Flash\n");
            break;
        case BLACK_WIDOW:       
            printf("Black Widow\n");
            break;
        case CAPTAIN_AMERICA:
            printf("Captain America\n");
            break;
        case HAWKEYE:
            printf("Hawkeye\n");
            break;
        case IRONMAN:
            printf("Iron Man\n");
            break;
        case SPIDERMAN:
            printf("Spiderman\n");
            break;
        case THE_HULK:
            printf("The Hulk\n");
            break;
        
        default:
            printf("Unknown Hero\n");
    }
}




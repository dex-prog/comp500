#include <iostream>

int main() {
    int n;
    
    std::cout << "Enter the number of terms:" << std::endl;
    std::cin >> n;
    
    if (n <= 0) {
        std::cout << "Please enter a positive integer." << std::endl;
        return 1;
    }
    
    std::cout << "Fibonacci Series: ";
    
    if (n >= 1) {
        std::cout << "0";
    }
    
    if (n >= 2) {
        std::cout << " 1";
    }
    
    if (n > 2) {
        int prev1 = 0;  
        int prev2 = 1;  
        
        for (int i = 3; i <= n; i++) {
            int current = prev1 + prev2; 
            std::cout << " " << current;
            
            prev1 = prev2;
            prev2 = current;
        }
    }
    
    std::cout << std::endl;
    
    return 0;
}
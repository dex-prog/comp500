#include <iostream>

int gcd(int a, int b) {
    while (b != 0) {
        int remainder = a % b;
        a = b;
        b = remainder;
    }
    return a;
}

int lcm(int a, int b) {
    int gcd_value = gcd(a, b);
    
    return (a / gcd_value) * b;
}

int main() {
    int num1, num2;
    
    std::cout << "Enter two numbers:" << std::endl;
    std::cin >> num1 >> num2;
    
    if (num1 <= 0 || num2 <= 0) {
        std::cout << "Please enter positive integers only." << std::endl;
        return 1;
    }
    
    int result = lcm(num1, num2);
    std::cout << "LCM: " << result << std::endl;
    
    return 0;
}
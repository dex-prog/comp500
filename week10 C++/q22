#include <iostream>

int main() {
    int num1, num2;
    
    std::cout << "Enter two numbers:" << std::endl;
    std::cin >> num1 >> num2;
    
    if (num1 <= 0 || num2 <= 0) {
        std::cout << "Please enter positive integers only." << std::endl;
        return 1;
    }
    

    while (num2 != 0) {
        int remainder = num1 % num2;  
        num1 = num2;                  
        num2 = remainder;              
    }
    
    std::cout << "GCD: " << num1 << std::endl;
    
    return 0;
}
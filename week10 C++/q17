#include <iostream>
int main() {
    int arr[5];

    std::cout << "Enter 5 numbers: " << std::endl;
    for (int i = 0; i < 5; i++) {
        std::cin >> arr[i];
    }

        int sum = 0;

    for (int i = 0; i < 5; i++) {
        sum += arr[i];
    }
    
    double avg = (double) sum / 5;
    std::cout << "Average of the array: " << avg << std::endl;
    return 0;
}
